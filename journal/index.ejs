<%- partial("_shared/_header") %>

<section class="container">
  <div class="row">
    <div class="column column">
      <% for(var slug in public.journal.posts._data){ %>
        <h1 class="f2"><a href="posts/<%= slug %>" id="blogtitle"><%= public.journal.posts._data[slug].title %><br />
        <span class="f3"><%= public.journal.posts._data[slug].date %></span></a></h1>
        <p class="f4"><%= public.journal.posts._data[slug].description %></p>
      <% }; %>
    </div>

        <div class="column blognav">
          <% for(var slug in public.journal.posts._data){ %>
            <h1 class="f3"><a href="posts/<%= slug %>"><%= public.journal.posts._data[slug].title %><br />
            <span class="f4"><%= public.journal.posts._data[slug].date %></span></a></h1>
          <% }; %>
        </div>
  </div>
</section>
